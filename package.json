    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gas-webpack-plugin": "^2.1.0",
    "jest": "^27.3.1",
    "npm-watch": "^0.11.0",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
